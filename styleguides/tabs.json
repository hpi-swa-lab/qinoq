n            owner.showContent(this.content);\n          }\n          this.setProperty('selected', selected);\n          this.setAppearance(selected);\n        }\n      },\n      closeable: {\n        defaultValue: true,\n        set (closeable) {\n          this.setProperty('closeable', closeable);\n          if (closeable) {\n            this.addMorph(this.closeIcon);\n          } else {\n            this.closeIcon = this.getSubmorphNamed('tab close');\n            this.closeIcon.remove();\n          }\n        }\n      }\n    };\n  }\n\n  setAppearance (isSelected) {\n    if (isSelected) {\n      this.master = $world.getSubmorphNamed('tab/selected');\n    } else {\n      this.master = {\n        auto: $world.getSubmorphNamed('tab/default'),\n        click: $world.getSubmorphNamed('tab/active'),\n        hover: $world.getSubmorphNamed('tab/hover')\n      };\n    }\n  }\n\n  isTab () {\n    return true;\n  }\n\n  close () {\n    this.getOwnerNamed('tabs').selectNearestTab(this);\n    this.remove();\n  }\n\n  onMouseUp () {\n    if (!this.selected) this.selected = true;\n  }\n\n  newMethod () {}\n}\n\n\n"},"CloseTabButton":{"package.json":"{\n  \"name\": \"CloseTabButton\",\n  \"version\": \"0.1.1-0\",\n  \"lively\": {\n    \"isObjectPackage\": true\n  }\n}","index.js":"import { Label } from \"lively.morphic\";\n\nexport default class CloseTabButton extends Label {\n  onMouseUp () {\n    this.owner.close();\n  }\n}\n\n\n"},"Tabs":{"package.json":"{\n  \"name\": \"Tabs\",\n  \"version\": \"0.1.1-2\",\n  \"lively\": {\n    \"isObjectPackage\": true\n  },\n  \"dependencies\": {},\n  \"devDependencies\": {},\n  \"main\": \"index.js\"\n}","index.js":"import { Morph } from \"lively.morphic/morph.js\";\nimport { pt } from \"lively.graphics/index.js\";\nimport { resource } from \"lively.resources/src/helpers.js\";\n\nexport default class Tabs extends Morph {\n  setBounds (bounds) {\n    super.setBounds(bounds);\n    const contentContainer = this.getSubmorphNamed('tab content container');\n    const flapsContainer = this.getSubmorphNamed('tab flap container');\n\n    contentContainer.extent = pt(bounds.width, bounds.height - flapsContainer.height);\n    contentContainer.position = pt(0, flapsContainer.top + flapsContainer.height);\n  }\n\n  async addTab (caption, content = null) {\n    const newTab = await resource('part://$world/tab').read();\n    newTab.caption = caption;\n    newTab.content = content;\n\n    const tabFlapContainer = this.getSubmorphNamed('tab flap container');\n    const newTabButton = this.getSubmorphNamed('button new tab');\n    tabFlapContainer.addMorph(newTab, newTabButton);\n\n    newTab.selected = true;\n    return newTab;\n  }\n\n  deselectAllTabs () {\n    this.getSubmorphNamed('tab flap container').tabs.forEach(tab => {\n      tab.selected = false;\n    });\n  }\n\n  selectNearestTab (otherTab) {\n    const tabs = this.getSubmorphNamed('tab flap container').tabs;\n    if (tabs.length === 0) return;\n    if (tabs.length === 1) {\n      tabs[0].selected = true;\n      return;\n    }\n    let tabIndex = tabs.indexOf(otherTab);\n    const tab = tabIndex < tabs.length - 1 ? tabs[++tabIndex] : tabs[--tabIndex];\n    tab.selected = true;\n    return tab;\n  }\n\n  showContent (content) {\n    const container = this.getSubmorphNamed('tab content container');\n    container.withAllSubmorphsDo(submorph => {\n      if (submorph !== container) submorph.remove();\n    });\n\n    if (content) {\n      container.addMorph(content);\n      content.position = pt(0, 0);\n      content.extent = container.extent;\n    }\n  }\n}\n\n\n"},"TabFlapContainer":{"package.json":"{\n  \"name\": \"TabFlapContainer\",\n  \"version\": \"0.1.1-0\",\n  \"lively\": {\n    \"isObjectPackage\": true\n  }\n}","index.js":"import { Morph } from \"lively.morphic\";\n\nexport default class TabFlapContainer extends Morph {\n  get tabs () {\n    return this.submorphs.filter(submorph => submorph.isTab);\n  }\n}\n\n\n"},"AddTabButton":{"package.json":"{\n  \"name\": \"AddTabButton\",\n  \"version\": \"0.1.1-0\",\n  \"lively\": {\n    \"isObjectPackage\": true\n  }\n}","index.js":"import { Morph } from \"lively.morphic\";\n\nexport default class AddTabButton extends Morph {\n  async onMouseUp () {\n    await this.addNewTab();\n  }\n\n  async addNewTab () {\n    this.getOwnerNamed('tabs').addTab('Unnamed Tab');\n  }\n}\n\n\n"}}},"packageDepMap":{"local://lively-object-modules/Tab":[],"local://lively-object-modules/CloseTabButton":[],"local://lively-object-modules/Tabs":[],"local://lively-object-modules/TabFlapContainer":[],"local://lively-object-modules/AddTabButton":[]},"preview":""}